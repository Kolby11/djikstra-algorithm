{
	"tabWidth": 2,
	"printWidth": 120,
	"singleQuote": true,
	"trailingComma": "es5",
	"semi": false,
	"parser": "typescript",
	"arrowParens": "avoid",
	"overrides": [
		{
			"files": "*.svelte",
			"options": {
				"parser": "svelte"
			}
		}
	],
	"pluginSearchDirs": false,
	"plugins": [
		"prettier-plugin-svelte",
		"prettier-plugin-organize-attributes",
		"prettier-plugin-organize-imports",
		"prettier-plugin-tailwindcss"
	]
}
